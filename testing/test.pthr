

var global: Bool = true;

func get_global_ptr = () -> Bool^ {
	return (addr global);
}


func entry = () #entry -> Int {
	var foo: Int = 0;

	if(get_global_ptr().^){
		@__printHelloWorld();
		foo = 12;

	}else if(true){
		foo = 18;

	}else{
		@__printHelloWorld();
		@__printHelloWorld();
	}

	return foo;
}


